<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.group.sharegram.schedule.mapper.EarlyMapper">
	
	<insert id="insertEarly" parameterType="EarlyDTO">	
		INSERT INTO 
	   		EARLY
	       		(EMP_NO, EARLY_CONTENT, EARLY_DATE)
	    	VALUES
	       		(#{empNo}, #{earlyContent}, #{earlyDate} )
	</insert>
	
</mapper>